config:
  # GitHub repository in format "owner/repo"
  vercel:githubRepo: "your-username/your-website-repo"

  # Optional: custom domain (without www)
  # vercel:domain: "yourdomain.com"

  # Optional: production branch (default: main)
  # vercel:productionBranch: "main"

  # Vercel provider configuration
  # Set via: pulumi config set --secret vercel:apiToken "your-token"
  vercel:apiToken: ""
